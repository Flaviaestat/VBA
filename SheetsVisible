Sub Exibe_Plans()
'
' Exibe_Plans Macro
'
    Sheets("Coeficientes").Visible = True
    Sheets("Coeficientes (MA)").Visible = True
    
End Sub

Sub Guarda_Plans()
    Sheets("Coeficientes").Visible = False
    Sheets("Coeficientes (MA)").Visible = False
   
End Sub


